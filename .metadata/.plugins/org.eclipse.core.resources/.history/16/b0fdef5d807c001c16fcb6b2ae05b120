package application;

import java.util.ArrayList;
import java.util.List;

public class Listas {

	public static void main(String[] args) 
	{
		List <String> list = new ArrayList<>();// lista não aceita tipo primitos, é necessário usar a wrapper class. Nesse caso a Integer

		list.add("Maria");
		list.add("Alex");
		list.add("Ana");
		list.add(2, "Joao");
		
		for (String x : list) {
			System.out.println(x);
		}
		
		
	}

}
